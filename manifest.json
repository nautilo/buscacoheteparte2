{
  "manifest_version": 3,
  "name": "BuscaCohete",
  "version": "1.0",
  "description": "Buscador seguro",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "permissions": [
    "activeTab",
    "scripting",
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "storage",
    "tabs",
    "history",
    "webNavigation",
    "webRequest",
    "notifications",
    "identity"
  ],
  "host_permissions": [
    "<all_urls>",
    "chrome-extension://ebbplnackfijdgaocobghipflkadnejp/*",
    "chrome-extension://ebbplnackfijdgaocobghipflkadnejp/perfiles.html"

  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon16.png",
    "128": "images/icon16.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon16.png"
  },
  "options_page": "./options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js", "content.js"],
      "run_at": "document_idle"
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "block-all-content",
        "enabled": true,
        "path": "block_rules.json"
      }
    ]
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "web_accessible_resources": [
    {
      "resources": ["styles.css", "Chart.min.js "],
      "matches": ["<all_urls>"]
    }
  ],
  "key": ""
}
